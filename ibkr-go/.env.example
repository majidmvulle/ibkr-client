# App
APP_NAME=ibkr-client
APP_ENV=local
APP_DEBUG=true
APP_GRPC_PORT=50051
APP_HTTP_PORT=8080
APP_VERSION=v0.0.1

# Logs (LevelDebug = -4, LevelInfo = 0, LevelWarn = 4, LevelError = 8)
APP_LOG_LEVEL=0

# Database
DB_WRITE_DSN=postgres://postgres:password@localhost:5434/ibkr?sslmode=disable
DB_READ_DSN=postgres://postgres:password@localhost:5434/ibkr?sslmode=disable
DB_LOG_LEVEL=2

# IBKR Client Portal Gateway (for docker-compose)
IBKR_USERNAME=your_username
IBKR_PASSWORD=your_password
IBKR_GATEWAY_URL=http://localhost:5000
IBKR_GATEWAY_KEY_PATH=/path/to/key.pem
IBKR_ACCOUNT_ID=your_account_id

# mTLS Authentication (for service-to-service)
MTLS_ENABLED=true
MTLS_CA_CERT_PATH=/path/to/ca.pem
MTLS_SERVER_CERT_PATH=/path/to/server-cert.pem
MTLS_SERVER_KEY_PATH=/path/to/server-key.pem

# Encryption (AES-256 key for session token encryption - 32 bytes base64 encoded)
ENCRYPTION_KEY=generate_with_openssl_rand_base64_32

# OpenTelemetry
OTEL_COLLECTOR_ENDPOINT=http://your-otel-collector:4317
