# Test Environment Configuration Example
# Copy this file to .env.test and update with your values

# Application
APP_NAME=ibkr-client
APP_ENV=test
APP_DEBUG=true
APP_VERSION=test
APP_HTTP_PORT=8080
APP_GRPC_PORT=50051
APP_LOG_LEVEL=-4

# Database
DB_WRITE_DSN=postgres://ibkr:ibkr_test_password@localhost:5432/ibkr_test?sslmode=disable
DB_READ_DSN=postgres://ibkr:ibkr_test_password@localhost:5432/ibkr_test?sslmode=disable
DB_LOG_LEVEL=4

# Encryption (32 bytes for AES-256)
ENCRYPTION_KEY=12345678901234567890123456789012

# IBKR Gateway
IBKR_GATEWAY_URL=http://localhost:5000
IBKR_ACCOUNT_ID=DU123456

# mTLS
MTLS_ENABLED=false
MTLS_CA_CERT_PATH=
MTLS_SERVER_CERT_PATH=
MTLS_SERVER_KEY_PATH=

# OpenTelemetry
OTEL_COLLECTOR_ENDPOINT=
